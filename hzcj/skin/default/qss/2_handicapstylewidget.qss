HandicapStyledWidget
{
qproperty-riseColor:#f75245;
qproperty-downColor:#0fc351;
qproperty-textColor:#ffffff;
qproperty-textFamily:"ArialMT";
qproperty-textPixelSize:13;
qproperty-textBold:true;
qproperty-risePriceColor:#e62424;
qproperty-downPriceColor:#0fc351;
qproperty-volumeColor:#cad4dc;
}

BuySellFiveView
{
background:transparent;
border:none;
}

BuySellFiveView#buyfiveview::item{
