background:#01240d;
}

HandicapTitleWidget
{
background:#191919;
}


HandicapTitleWidget QLabel#name
{
color:#cad4dc;
font:bold 16px "SimSun";
border:none;
}

HandicapTitleWidget QLabel#code
{
color:#cad4dc;
font: bold 20px "Arial";
