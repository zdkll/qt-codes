height:26px;
border-radius: 1px;
}

StockDetailQuotePanelWnd  CSubTab#sletTab QPushButton:checked
{
background-color:#3c3c3c;
color:#ffffff;
}

BlockDetaillistWnd QPushButton#upBt
{
    background-image:url(:/skin/default/img/stockdetail/up02.png);
    background-repeat:none;
    background-position: center;
    border:none;
}

BlockDetaillistWnd QPushButton#upBt:hover
{
    background-image:url(:/skin/default/img/stockdetail/up02.png);
}

BlockDetaillistWnd QPushButton#upBt:checked
{
    background-image:url(:/skin/default/img/stockdetail/right03_hover.png);
}

BlockDetaillistWnd QPushButton#downBt
{
    background-image:url(:/skin/default/img/stockdetail/down02.png);
    background-repeat:none;
    background-position: center;
    border:none;
}

BlockDetaillistWnd QPushButton#downBt:hover
{
    background-image:url(:/skin/default/img/stockdetail/down02.png);
}

BlockDetaillistWnd QPushButton#downBt:checked
{
    background-image:url(:/skin/default/img/stockdetail/down02_hover.png);
}

