}

/*标题选择*/
StockBaseDetailPage CSubTab#seltTab QPushButton
{
background-color:transparent;
font-size:12px;
font-family:"SimSun";
color:#797979;

width:80px;
padding-top:4px;
padding-bottom:4px;
}

StockBaseDetailPage CSubTab#seltTab QPushButton:checked
{
color:#ffffff;
}


/*F10*/
StockBaseDetailPage QPushButton#bottomF10
{
    background-color:#222222;
    font-size:12px;
    font-family:"SimSun";
    color:#adadad;
    border-radius: 2px;
    width:70px;
    padding-top:4px;
    padding-bottom:4px;
}

StockBaseDetailPage QPushButton#bottomF10::hover
{
    background-color:#414141;
    color:#ffffff;
}



