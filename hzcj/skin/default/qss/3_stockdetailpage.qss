StockDetailPage
{
    background-color:#010101;
}

/***************************左侧区域 自选 资金异动，最近浏览*******/
StockDetailListWnd QWidget#titleWnd
{
background-color:#191919;
}

/*选择控件*/
StockDetailListWnd CSubTab#sletTab QPushButton
{
background-color:transparent;
font-size:12px;
font-family:"SumSun";
color:#797979;

padding-top:4px;
padding-bottom:4px;
padding-left:10px;
padding-right:10px;
}

StockDetailListWnd CSubTab#sletTab QPushButton:checked
{
color:#ffffff;
}

/*板块详情左侧窗口标题样式*/
BlockDetaillistWnd  QLabel#optionLabel
{
    background-color:#222222;
    font-size:12px;
    font-family:"SimSun";
    color:#ffffff;
    padding:0px;
}

/***************************左侧区域***************************/

/***************************中间区域  K线分时样式***************************/
StockDetailKlineMinWnd QWidget#titleWnd
{
background-color:#191919;
}

StockDetailKlineMinWnd HZLine
{
   color:#010101;
}

/*股票名称*/
StockDetailKlineMinWnd QLabel#objName
{
font:bold 13px "Arial";
color:#ffffff;
padding-left:10;
}

/*周期选择控件*/
StockDetailKlineMinWnd CSubTab#periodTab QPushButton{
background-color:transparent;
font-size:12px;
font-family:"SimSun";
color:#adadad;
border-radius: 2px;
width:60px;
padding-top:4px;
padding-bottom:4px;
}

StockDetailKlineMinWnd CSubTab#periodTab QPushButton:checked
{
background-color:#414141;
color:#ffffff;
}

/*指标选择控件*/
StockDetailKlineMinWnd QWidget#midIndiPanel
{
    background-color:#0f1317;
    border-top:1px solid #2f2f2f;
    border-bottom:1px solid #2f2f2f;
}

StockDetailKlineMinWnd CSubTab >HZLine
{
    color:#2f2f2f;
}

StockDetailKlineMinWnd CSubTab#indiTab
{
background-color:#0f1317;
border-top:1px solid #2f2f2f;
border-bottom:1px solid #2f2f2f;
}


StockDetailKlineMinWnd CSubTab#indiTab QPushButton
{
background-color:transparent;
font-size:12px;
font-family:"Arial";
color:#adadad;

padding-top:4px;
padding-bottom:4px;
padding-left:10px;
padding-right:10px;
}

StockDetailKlineMinWnd CSubTab#indiTab QPushButton:checked
{
color:#4f9bff;
}


/*左侧隐藏显示按钮*/
StockDetailKlineMinWnd QToolButton#leftExpandButton
{
image:url(:/skin/default/img/expansion/expansion-hover01.png);
image-position:center;
border:none;
}

StockDetailKlineMinWnd QToolButton#leftExpandButton:checked
{
image:url(:/skin/default/img/expansion/expansion01.png);
}

/*右侧隐藏显示按钮*/
StockDetailKlineMinWnd QToolButton#rightExpandButton
{
image:url(:/skin/default/img/expansion/expansion-hover02.png);
image-position:center;
border:none;
}

StockDetailKlineMinWnd QToolButton#rightExpandButton:checked
{
image:url(:/skin/default/img/expansion/expansion02.png);
}


/***************************中间区域 样式 end***************************/

/**************************页面导航 样式***************************/
StockBaseDetailPage QWidget#titleWnd
{
    background-color:#222222;
}

StockBaseDetailPage HZLine
{
   color:#010101;
}

/*底部隐藏显示按钮*/
StockBaseDetailPage QToolButton#bottomExpandButton
{
image:url(:/skin/default/img/expansion/expansion-hover03.png);
image-position:center;
border:none;
}

StockBaseDetailPage QToolButton#bottomExpandButton:checked
{
