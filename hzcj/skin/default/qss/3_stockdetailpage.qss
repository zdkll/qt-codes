/**************************右侧盘口   样式***************************/
/*盘口五档买卖标题*/
QuotePanel QWidget#title
{
    background-color:#2f2f2f;
}

QuotePanel QWidget#mainwidget
{
    background-color:transparent;
}

QuotePanel QLabel
{
    font: 12px "SimSun";
    color:#ffffff;
    padding-left:15;
}

QuotePanel QLabel#tickPrice
{
    padding-left:7;
}

QuotePanel QPushButton#clickTick
{
width:18px;
height:18px;
border-image:url(:/skin/default/img/handicap/detailed.png);
}

QuotePanel QPushButton#clickTick:hover
{
border-image:url(:/skin/default/img/handicap/detailed_hover.png);
}

QuotePanel QPushButton#clickTick::pressed{
border-image:url(:/skin/default/img/handicap/detailed_hover.png);
}

StockDetailQuotePanelWnd
{
    background-color:#000000;
}

StockDetailQuotePanelWnd QWidget#QuoteTile
{
    background-color:red;
}

StockDetailQuotePanelWnd QWidget#ExtraDataWnd
{
    background-color:blue;
}


StockDetailQuotePanelWnd QWidget#BaseDataWnd
{
    background-color:yellow;
    border-top:1px solid #222222;
    border-bottom:1px solid #222222;
}

/*选择控件*/
