/****************************************************************
name_id:ScrollWidget
description:可滚动窗口
*******************************************************************/
ScrollWidget QScrollArea
{
border:none;
background:black;
}

ScrollWidget QScrollArea
{
background:black;
}

/****************scrollbar   vertical**************************/
ScrollWidget  QScrollBar:vertical
{
width:8px;
background-color:#0e0e0e;
margin-top:14px;
margin-bottom:14px;
}

ScrollWidget  QScrollBar::handle:vertical
{
background-color:#2e2e2e;
border-radius:0px;
min-height:  20px;
margin-left:0px;
margin-right:1px;
}

ScrollWidget  QScrollBar::handle:vertical:hover
{
background: #505050;
}

ScrollWidget  QScrollBar::add-page:vertical
{
background-color:#0e0e0e;
}

ScrollWidget   QScrollBar::sub-page:vertical
{
background-color:#0e0e0e;
}

ScrollWidget   QScrollBar::sub-line:vertical
{
background-color:#0e0e0e;
height: 14px;
subcontrol-position: top;
subcontrol-origin:margin;
}

ScrollWidget   QScrollBar::add-line:vertical
{
background-color:#0e0e0e;
height: 14px;
subcontrol-position: bottom;
subcontrol-origin:margin;
}

ScrollWidget   QScrollBar::up-arrow:vertical
{
image:url(:/skin/default/img/tableview/scrollbar_up.png);
}

ScrollWidget   QScrollBar::down-arrow:vertical
{
image:url(:/skin/default/img/tableview/scrollbar_down.png);
}


ScrollWidget QScrollBar:horizontal
{
    background:#0e0e0e;
    height:16px;
    margin:0px,0px,0px,0px;
    padding-top:4px;
    padding-bottom:3px;
    padding-left:20px;
    padding-right:20px;
}

ScrollWidget QScrollBar::handle:horizontal
{
    min-width:80px;
    height:10px;
    background:#2e2e2e;
    border-radius:3px;
}

ScrollWidget QScrollBar::handle:horizontal:hover
{
    background:#505050;
    border-radius:3px;
}

ScrollWidget QScrollBar::add-line:horizontal
{
    image:url(:/skin/default/img/tableview/scrollbar_right.png);
}

ScrollWidget QScrollBar::sub-line:horizontal
{
    image:url(:/skin/default/img/tableview/scrollbar_left.png);
}

ScrollWidget QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
    background: none;
}

