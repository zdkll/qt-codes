/*********************短线异动表格样式***********************/
ShortTermChangeView
{
    color: #adadad;
    font:  14px "SimSun";
    background-color: #191919;               /*表格内背景色*/
    selection-background-color: #2e2e2e;        /*选中区域的背景色*/
    border-radius: 0px;
    margin-top: 1px;
    margin-right: 1px;
    selection-color: #ffffff;
}

/*垂直滚动条整体*/
ShortTermChangeView QScrollBar:vertical
{
    width:15px;
    background:rgb(14,14,14);
    margin:0px,0px,0px,0px;
    padding-top:12px;   /*上预留位置*/
    padding-bottom:12px;    /*下预留位置*/
}

/*滚动条中滑块的样式*/
ShortTermChangeView  QScrollBar::handle:vertical
{
background-color:#2e2e2e;
border-radius:3px;
margin-left: 3px;
min-height: 20px;
margin-right:3px;
}
ShortTermChangeView QScrollBar::add-line:vertical
{
height: 14px;
background-color:#0e0e0e;
subcontrol-position: bottom;
subcontrol-origin:margin;
}

ShortTermChangeView QScrollBar::sub-line:vertical
{
height: 14px;
background-color:#0e0e0e;
subcontrol-position: top;
subcontrol-origin:margin;
}

ShortTermChangeView  QScrollBar::up-arrow
{
image:url(:/skin/default/img/tableview/scrollbar_up.png);
}

ShortTermChangeView QScrollBar::down-arrow
{
image:url(:/skin/default/img/tableview/scrollbar_down.png);
}

/*当滚动条滚动的时候，上面的部分和下面的部分*/
ShortTermChangeView QScrollBar::add-page:vertical,ShortTermChangeView QScrollBar::sub-page:vertical
{
    background:rgb(0,0,0,10%);
    border-radius:4px;
}

/*自定义的属性设置对应的样式*/
StyledWidget {
        qproperty-colUpColor: #e62727;
        qproperty-colDownColor: #0aaa38;
        qproperty-colEqualColor: #cad4dc;
        qproperty-colFirBold:true;
        qproperty-colFirFontFamily: "ArialMT";
        qproperty-colFirPixelSize: 13;
        qproperty-colSecFontFamily: "SimSun";
        qproperty-colSecPixelSize: 14;
        qproperty-colThrFontFamily: "SimSun";
        qproperty-colThrPixelSize: 14;
        qproperty-colForBold:true;
        qproperty-colForFontFamily: "ArialMT";
        qproperty-colForPixelSize: 13;
}





