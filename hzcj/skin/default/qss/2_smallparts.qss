}


ExitDialog QPushButton#cancel
{
border-image:url(:/skin/default/img/1mainwindow/smallparts/cancel.bmp) 0 0 0 0 repeat;
}

ExitDialog QPushButton#cancel:hover
{
border-image:url(:/skin/default/img/1mainwindow/smallparts/cancel.bmp) 30 0 0 0 repeat;
}


ExitDialog QPushButton#cancel:pressed
