}



StrategyTitleWnd QPushButton
{
qproperty-maximumSize:100px 28px;
qproperty-minimumSize:100px 28px;
border-radius:5px;
color:#ffffff;
font:12px "SimSun";
background:#1c4676;
}


StrategyTitleWnd QLabel#zyx,
StrategyTitleWnd QLabel#cxx
{
qproperty-maximumSize:102px 18px;
qproperty-minimumSize:102px 18px;
}
/******重要性*******/
