/*************************右侧上方标题样式***************/
StrategyTitleWnd
{
     background-color:#191919;
}

StrategyTitleWnd QLabel
{
background:transparent;
color:#cad4dc;
font:14px "SimSun";
padding:0;
margin:0;
border:0;
}

StrategyTitleWnd QLabel#title
{
font:bold 24px "MiscrosoftYaHei";
}

StrategyTitleWnd QLabel#content
{
