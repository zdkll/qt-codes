/*************************今日机会样式界面 以及listview样式配置***************/
StrategyTodayChanceWnd
{
    background-color:#000000;
    qproperty-m_codeNameFontFamily:'SimSun';
    qproperty-m_codeNameFontSize:14;
    qproperty-m_codeNameTextClr: #ffffff;

    qproperty-m_descTextFontFamily:'SimSun';
    qproperty-m_descTextFontSize:12;
    qproperty-m_descTextTextClr: #adadad;

    qproperty-m_descTextFontFamily:'Arial';
    qproperty-m_descTextFontSize:13;

    qproperty-m_equalClr:#b1bbc3;
    qproperty-m_upClr:#e62727;
    qproperty-m_downClr:#0aaa38;

    qproperty-m_itemSelectedClr:#191919;
    qproperty-m_itemHoverClr:#253a54;
    qproperty-m_itemBorderClr:#2c2c2c;
}

StrategyTodayChanceWnd QLabel
{
    font: 12px "SimSun";
    color:#cad4dc;
    background:#1C1C1C;
}


StrategyTodayChanceWnd QScrollBar:vertical
{
    width:15px;
    background:rgb(14,14,14);
    margin:0px,0px,0px,0px;
    padding-top:12px;   /*上预留位置*/
    padding-bottom:12px;    /*下预留位置*/
}

/*滚动条中滑块的样式*/
 StrategyTodayChanceWnd QScrollBar::handle:vertical
{
    background-color:#2e2e2e;
    border-radius:3px;
    margin-left: 3px;
    min-height: 20px;
    margin-right:3px;
}
StrategyTodayChanceWnd QScrollBar::add-line:vertical
{
    height: 14px;
    background-color:#0e0e0e;
    subcontrol-position: bottom;
    subcontrol-origin:margin;
}

StrategyTodayChanceWnd QScrollBar::sub-line:vertical
{
    height: 14px;
    background-color:#0e0e0e;
    subcontrol-position: top;
    subcontrol-origin:margin;
}

StrategyTodayChanceWnd QScrollBar::up-arrow
{
    image:url(:/skin/default/img/tableview/scrollbar_up.png);
}

StrategyTodayChanceWnd QScrollBar::down-arrow
{
    image:url(:/skin/default/img/tableview/scrollbar_down.png);
}

/*当滚动条滚动的时候，上面的部分和下面的部分*/
StrategyTodayChanceWnd QScrollBar::add-page:vertical,#HZKBControl QScrollBar::sub-page:vertical
{
    background:rgb(0,0,0,10%);
    border-radius:4px;
}

StrategyTodayChanceWnd QListView {
    background-color: #222222;
    border: solid transparent;
}

/*************************今日机会样式listview样式配置 end***************/

