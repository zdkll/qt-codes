}


StrategyTitleWnd QLabel#zyx[type='3']
{
border-image:url(:/skin/default/img/1mainwindow/smallparts/Star05.png);
}


StrategyTitleWnd QLabel#zyx[type='4']
{
border-image:url(:/skin/default/img/1mainwindow/smallparts/Star06.png);
}

StrategyTitleWnd QLabel#zyx[type='5']
{
border-image:url(:/skin/default/img/1mainwindow/smallparts/Star05.png);
}

/******持续性*******/
